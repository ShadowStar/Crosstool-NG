CT_EXPERIMENTAL=y
CT_DEBUG_CT=y
CT_DEBUG_CT_SAVE_STEPS=y
CT_LOCAL_TARBALLS_DIR="${HOME}/src"
CT_SAVE_TARBALLS=y
CT_WORK_DIR="${CT_TOP_DIR}/tmp"
# CT_RM_RF_PREFIX_DIR is not set
# CT_REMOVE_DOCS is not set
# CT_INSTALL_DIR_RO is not set
CT_CONNECT_TIMEOUT=3
CT_USE_MIRROR=y
CT_MIRROR_BASE_URL="http://mirrors.163.com/gentoo/distfiles"
# CT_OVERIDE_CONFIG_GUESS_SUB is not set
CT_PARALLEL_JOBS=9
CT_LOG_EXTRA=y
CT_LOG_SEE_TOOLS_WARN=y
CT_ARCH_ARCH="i686"
CT_ARCH_x86=y
CT_TARGET_VENDOR="pc"
CT_KERNEL_linux=y
CT_KERNEL_V_2_6_32=y
CT_BINUTILS_LINKER_LD_GOLD=y
CT_BINUTILS_LD_WRAPPER=y
CT_BINUTILS_PLUGINS=y
CT_LIBC_glibc=y
CT_LIBC_GLIBC_V_2_12_2=y
CT_LIBC_GLIBC_EXTRA_CONFIG_ARRAY="--enable-multi-arch"
CT_LIBC_GLIBC_EXTRA_CFLAGS="-O2"
CT_LIBC_ADDONS_LIST="libidn"
CT_CC_LANG_CXX=y
# CT_CC_STATIC_LIBSTDCXX is not set
CT_CC_GCC_SYSTEM_ZLIB=y
CT_CC_GCC_LIBMUDFLAP=y
CT_CC_GCC_LIBGOMP=y
CT_CC_GCC_LIBSSP=y
CT_CC_GCC_LIBQUADMATH=y
CT_CC_GCC_DISABLE_PCH=y
CT_CC_GCC_BUILD_ID=y
CT_DEBUG_gdb=y
# CT_GDB_CROSS_PYTHON is not set
CT_GDB_CROSS_EXTRA_CONFIG_ARRAY="--disable-sim"
# CT_GDB_GDBSERVER_STATIC is not set
CT_GDB_V_7_7=y
