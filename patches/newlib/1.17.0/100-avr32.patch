+clock_t __attribute__((weak))
+     _times( struct tms *tms )
+{
+  int tick;
+  tick = __builtin_mfsr(SYSREG_COUNT);
+  tms->tms_utime = 0;
+  tms->tms_stime = tick;
+  tms->tms_cutime = 0;
+  tms->tms_cstime = 0;
+  return tick;
+}
